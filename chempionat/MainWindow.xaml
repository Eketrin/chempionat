<Window x:Class="chempionat.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:chempionat"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="But1" Content="Добавить" HorizontalAlignment="Left" Margin="276,68,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="But1_Click"/>
        <Button x:Name="But2" Content="Рассчитать результаты" HorizontalAlignment="Left" Margin="508,270,0,0" VerticalAlignment="Top" Width="151" Click="But2_Click"/>
        <TextBox x:Name="Input" HorizontalAlignment="Left" Height="23" Margin="48,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="196" TextChanged="Input_TextChanged"/>
        <ListBox x:Name="Spisok" HorizontalAlignment="Left" Height="263" Margin="48,108,0,0" VerticalAlignment="Top" Width="303" SelectionChanged="Spisok_SelectionChanged"/>
        <TextBlock x:Name="Result" HorizontalAlignment="Left" Margin="426,68,0,0" TextWrapping="Wrap" Text="Результаты участников" VerticalAlignment="Top" Height="184" Width="306"/>


    </Grid>
</Window>
